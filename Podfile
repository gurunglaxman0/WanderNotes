# Podfile
use_frameworks!

target 'WanderNotes' do
pod 'RxSwift'
pod 'RxCocoa'
pod 'Moya'
end

# RxTest and RxBlocking make the most sense in the context of unit/integration tests

target 'WanderNotesTests' do
pod 'RxBlocking', '~> 4.0'
pod 'RxTest',     '~> 4.0'
pod 'Moya'
end

target 'WanderNotesUITests' do
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    pod 'Moya'
end
